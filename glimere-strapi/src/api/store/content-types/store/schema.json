{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "store",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "storeId": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "location": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "socials": {
      "type": "json"
    },
    "established_year": {
      "type": "integer"
    },
    "reviews": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::review.review",
      "inversedBy": "stores"
    },
    "follows": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::follow.follow",
      "mappedBy": "stores"
    }
  }
}
